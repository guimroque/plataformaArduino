<%- include ("../partials/header.ejs") %>
<%- include ("../partials/homenavbar.ejs") %>

<body>
    <div class="jumbotron justify-contet-center" style="margin-bottom:0px; border-radius: 0; padding: 5px;">
    
        <div class="container w-50 card mt-5 mb-5" style="min-height:100px;">
            
            <div class="container">
                
                    <form action="" style="width: 100%;">
                        <div class="input-group mb-3 mt-3">
                            <input type="text" class="form-control" placeholder="O que voce procura?">
                            <div class="input-group-append">
                              <button class="btn btn-outline-dark" type="submit" >Pesquisar</button>
                            </div>
                          </div>
                    </form>
                 <% dino.forEach((dino)=>{%>
                  <div class="row justify-contet-center">
                  <div class="col" style="width: 10%;">
                  </div>
                  <div class="col-6 text-center" style="min-height:100px;min-width:80%;">
                    
                    <a href="/user/dino/<%=dino.id%>" style="text-decoration:none; color:black" >
                        <div class="card-header" style="border-radius: 5px; min-height: 130px;">
                            <img src="/img/<%=dino.img%>" style="width: 20%;">
                            <p><b><%=dino.nome%></b></p>
                        </div>
                    </a>
                  </div>
                  <div class="col" style="min-width: 10%;">
                  </div>
                </div> <br>
                 <% }) %>
                 <% componente.forEach((componente)=>{%>
                  <div class="row justify-contet-center">
                  <div class="col" style="width: 10%;">
                  </div>
                  <div class="col-6 text-center" style="min-height:100px;min-width:80%;">
                    
                    <a href="/user/componente/<%=componente.id%>" style="text-decoration:none; color:black" >
                        <div class="card-header" style="border-radius: 5px; min-height: 130px;">
                            <img src="/img/<%=componente.capa%>" style="width: 20%;">
                            <p><b><%=componente.nome%></b></p>
                        </div>
                    </a>
                  </div>
                  <div class="col" style="min-width: 10%;">
                  </div>
                </div> <br>
                 <% }) %>
                 <% humano.forEach((humano)=>{%>
                  <div class="row justify-contet-center">
                  <div class="col" style="width: 10%;">
                  </div>
                  <div class="col-6 text-center" style="min-height:100px;min-width:80%;">
                    
                    <a href="/user/humano/<%=humano.id%>" style="text-decoration:none; color:black" >
                        <div class="card-header" style="border-radius: 5px; min-height: 130px;">
                            <img src="/img/<%=humano.img%>" style="width: 20%;">
                            <p><b><%=humano.nome%></b></p>
                        </div>
                    </a>
                  </div>
                  <div class="col" style="min-width: 10%;">
                  </div>
                </div> <br>
                 <% }) %>
        
    
    </div>


</body>


  
<%- include ("../partials/footer.ejs") %>